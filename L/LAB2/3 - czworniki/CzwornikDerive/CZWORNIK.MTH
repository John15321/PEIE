ZWEJ(a,z):=ELEMENT(a,1,1)-ELEMENT(a,1,2)*ELEMENT(a,2,1)/(ELEMENT(a,2,2)+z)

ZWYJ(a,z):=ELEMENT(a,2,2)-ELEMENT(a,1,2)*ELEMENT(a,2,1)/(ELEMENT(a,1,1)+z)

KU(a,z):=ELEMENT(a,2,1)*z/(DET(a)+ELEMENT(a,1,1)*z)

KI(a,z):=-ELEMENT(a,2,1)/(ELEMENT(a,2,2)+z)

KUSK(a,g,o):=ELEMENT(a,2,1)*o/(DET(a)+ELEMENT(a,1,1)*o+g*(ELEMENT(a,2,2)+o))

KPSK(a,g,o):=4*RE(g)*RE(o)*ABS(KUSK(a,g,o))^2/ABS(o)^2

MACZS(z):=[[1,z],[0,1]]

MACZR(z):=[[1,0],[1/z,1]]

MZCOL(x,y):=[[x+y,y],[y,y]]

MZCT(x,y,z):=[[x+y,y],[y,y+z]]

MZCK(w,x,y,z):=[[(w+z)*(x+y),y*z-w*x],[y*z-w*x,(w+y)*(x+z)]]/(w+x+y+z)

MZCP(x,y,z):=[[x*(y+z),x*z],[x*z,z*(x+y)]]/(x+y+z)

MZCTZ(w,x,y,z):=[[w*(y+z)+x*(w+y+z),w*y+x*(w+y+z)],[w*y+x*(w+y+z),y*(w+z)+x*(w~
+y+z)]]/(w+y+z)

MZZYR(r):=[[0,-r],[r,0]]

