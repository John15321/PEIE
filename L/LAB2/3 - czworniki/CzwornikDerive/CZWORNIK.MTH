YZH(a):=[[1,-ELEMENT(a,1,2)],[ELEMENT(a,2,1),DET(a)]]/ELEMENT(a,1,1)

YZG(a):=[[DET(a),ELEMENT(a,1,2)],[-ELEMENT(a,2,1),1]]/ELEMENT(a,2,2)

AZZ(a):=[[ELEMENT(a,1,1),DET(a)],[1,ELEMENT(a,2,2)]]/ELEMENT(a,2,1)

AZY(a):=[[ELEMENT(a,2,2),1],[DET(a),ELEMENT(a,1,1)]]/(-ELEMENT(a,2,1))

AZH(a):=[[DET(a),ELEMENT(a,1,1)],[ELEMENT(a,2,2),1]]/(-ELEMENT(a,2,1))

AZG(a):=[[1,ELEMENT(a,2,2)],[ELEMENT(a,1,1),DET(a)]]/ELEMENT(a,2,1)

HZZ(a):=[[DET(a),ELEMENT(a,1,2)],[-ELEMENT(a,2,1),1]]/ELEMENT(a,2,2)

