HZB(a):=[[ELEMENT(a,1,2),-1],[DET(a),ELEMENT(a,2,1)]]/(-ELEMENT(a,1,1))

BZZ(a):=[[ELEMENT(a,2,2),-DET(a)],[-1,ELEMENT(a,1,1)]]/ELEMENT(a,1,2)

BZY(a):=[[-ELEMENT(a,1,1),1],[DET(a),-ELEMENT(a,2,2)]]/ELEMENT(a,1,2)

BZH(a):=[[1,-ELEMENT(a,1,1)],[-ELEMENT(a,2,2),DET(a)]]/ELEMENT(a,1,2)

BZG(a):=[[-DET(a),ELEMENT(a,2,2)],[ELEMENT(a,1,1),-1]]/ELEMENT(a,1,2)

GZZ(a):=[[1,-ELEMENT(a,1,2)],[ELEMENT(a,2,1),DET(a)]]/ELEMENT(a,1,1)

GZY(a):=[[DET(a),ELEMENT(a,1,2)],[-ELEMENT(a,2,1),1]]/ELEMENT(a,2,2)

GZA(a):=[[ELEMENT(a,2,1),-DET(a)],[1,ELEMENT(a,1,2)]]/ELEMENT(a,1,1)

